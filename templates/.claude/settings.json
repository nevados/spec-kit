{
  "allowedCommands": [
    "git diff",
    "git status",
    "git log",
    "git show",
    "git branch",
    "git add",
    "git commit",
    "git push",
    "grep",
    "find",
    "ls",
    "cat",
    "head",
    "tail",
    "wc",
    "npm install",
    "npm ci",
    "python -m pip install",
    "cargo build",
    "go mod tidy"
  ],
  "allowedCommandPatterns": [
    "git diff*",
    "git log*",
    "git show*",
    "git add*",
    "git commit -m*",
    "git push*",
    "grep*",
    "find*",
    "ls*",
    "cat*",
    "npm run lint*",
    "npm test*",
    "npm run test*",
    "npm run build*",
    "pytest*",
    "cargo test*",
    "go test*",
    "python -m pytest*"
  ],
  "description": "Spec-Kit default command approvals - reduces friction for spec-driven development workflow"
}
